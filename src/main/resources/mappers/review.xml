<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="review">

	<parameterMap type="com.scglab.connect.services.wise.Wise" id="wise"></parameterMap>
	<resultMap type="com.scglab.connect.services.wise.Wise" id="wise"></resultMap>

	<insert id="regist" parameterType="map">
		insert into talk_review (company_id, gasapp_member_number, review_score)
		values(#{companyId}, #{gasappMemberNumber}, #{reviewScore})
	</insert>
	
	
</mapper>


